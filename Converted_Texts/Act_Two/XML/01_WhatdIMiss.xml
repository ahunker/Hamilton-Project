<?xml version="1.0" encoding="UTF-8"?><?xml-model href="../../../ODD/out/hamiltonODD.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?><?xml-model href="../../../ODD/out/hamiltonODD.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?><TEI xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader xml:lang="en">
        <fileDesc>
            <titleStmt>
                <title>
                    "What'd I Miss?"
                </title>
            </titleStmt>
            <publicationStmt>
                <p>
                    by Lin-Manuel Miranda
                </p>
            </publicationStmt>
            <sourceDesc>
                <p>
                    Lyrics provided resourced from http://atlanticrecords.com/HamiltonMusic/
                </p>
            </sourceDesc>
