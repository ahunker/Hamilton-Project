         <schemaSpec ident="starterODD" start="TEI" prefix="tei">
            <moduleRef key="tei"/>
            <moduleRef key="core" include="listBibl bibl title author editor publisher pubPlace biblScope name rs date desc head lg l lb note num p q ref resp respStmt sp speaker stage textLang otherLangs foreign"/>
            <moduleRef key="analysis" include="s cl phr w"/>
            <moduleRef key="drama" except="actor camera caption sound tech view prologue epilogue move performance"/>
            <moduleRef key="header"/>         
            <moduleRef key="namesdates" include="persName placeName roleName geo place listPlace"/>
            <moduleRef key="textstructure" include="TEI back body div front floatingText text"/>
            <constraintSpec scheme="isoschematron" ident="emotion-in-rs_ana">
               <constraint>
                  <sch:rule context="*[@type eq 'emotion']">
                     <sch:report test="not(@ana)">
                        rs type="emotion must have its part of speech coded in @ana!</sch:report>
                  </sch:rule>
               </constraint>
            </constraintSpec>	   
